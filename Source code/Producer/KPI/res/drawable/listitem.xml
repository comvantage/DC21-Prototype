<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="#fee0c4" />
        
		<stroke
	    	android:width="1dp"
	    	android:color="#fd6609"/>
    </shape>
  </item>
  <item>
    <shape>
      <solid
          android:color="#FFFFFF" />
      <padding
          android:left="5dp"
          android:top="5dp"
          android:right="5dp"
          android:bottom="5dp" />
    </shape>
  </item>
</selector>