<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kpi"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="15" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/kpilogo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.kpi.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
                
            </intent-filter>
            <intent-filter android:label="eu.comvantage.IAF" >
                <action android:name="eu.comvantage.iaf.DC21_KPI" />
                <category android:name="eu.comvantage.iaf.category.WORKFLOW" />
                <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>
        </activity>
        
        
        
        <activity android:name="com.example.kpi.KpiList" >
        </activity>        

        <activity android:name="com.example.kpi.kpi_details" >
        </activity>                
        
    </application>

</manifest>
